{
    "env": {
        "commonjs": true,
        "node": true
    },
    "extends": [
        "prettier",
        "airbnb-base",
        "airbnb-base/legacy"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": 0,
        "no-tabs": 0,
        "no-console": 0,
        "linebreak-style": 0,
        "operator-linebreak": [
            "error",
            "after",
            {
                "overrides": {
                    ":": "before",
                    "?": "before",
                    "||": "before"
                }
            }
        ],
        "no-underscore-dangle": 0,
        "import/no-unresolved": 0,
        "object-curly-newline": 0,
        "no-unused-vars": "warn",
        "class-methods-use-this": 0,
        "consistent-return": 0,
        "max-len": 0
    },
    "plugins": [
        "prettier"
    ]
}
